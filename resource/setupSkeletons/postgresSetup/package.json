		"jwt-decode": "^3.1.2",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"moment-timezone": "^0.5.33",
		"mongodb": "^3.6.9",
		"mongoose": "^5.13.0",
		"multer": "^1.4.4",
		"multer-s3": "^2.10.0",
