		"dotenv": "^10.0.0",
		"express": "^4.17.1",
		"express-session": "^1.17.3",
		"firebase-admin": "^9.11.0",
		"flat": "^5.0.2",
		"helmet": "^4.6.0",
		"https": "^1.0.0",
		"jest": "^27.0.6",
