{
	"name": "xxxxx-backend",
	"version": "1.0.0",
	"description": "Backend for the xxxxx app",
	"main": "app.js",
	"scripts": {
		"build": "npm install",
		"build-prod": "npm install",
		"start": "node app.js",
		"start-local": "nodemon --inspect app.js",
		"test": "jest",
		"migration-init": "npx sequelize-cli init",
		"migration-create": "npx sequelize-cli model:generate --name Test --attributes name:string",
		"migrate": "npx sequelize-cli db:migrate",
		"migrate-down": "npx sequelize-cli db:migrate:undo",
		"migrate-down-all": "npx sequelize-cli db:migrate:undo:all",
		"seed-create": "npx sequelize-cli seed:generate --name Test",
		"seed": "npx sequelize-cli db:seed:all",
		"seed-down": "npx sequelize-cli db:seed:undo",
		"seed-down-all": "npx sequelize-cli db:seed:undo:all"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@newrelic/winston-enricher": "^1.0.0",
		"aws-sdk": "^2.1065.0",
